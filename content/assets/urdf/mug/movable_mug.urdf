<?xml version="1.0"?>
<robot name="mug">
  <link name="tx"/>
  <link name="ty"/>
  <link name="tz"/>
  <link name="rx"/>
  <link name="ry"/>
  <link name="rz"/>
  <joint name="x_joint" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="tx"/>
    <child link="ty"/>
    <axis xyz="1 0 0"/>
    <limit effort="1" lower="-2" upper="2.0" velocity="0.1"/>
  </joint>
  <joint name="y_joint" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="ty"/>
    <child link="tz"/>
    <axis xyz="0 1 0"/>
    <limit effort="1" lower="-2.0" upper="2.0" velocity="0.1"/>
  </joint>
  <joint name="z_joint" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="tz"/>
    <child link="rx"/>
    <axis xyz="0 0 1"/>
    <limit effort="1" lower="-2.8973" upper="2.8973" velocity="0.1"/>
  </joint>
  
  <joint name="rx_joint" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="rx"/>
    <child link="ry"/>
    <axis xyz="1 0 0"/>
    <limit effort="12" lower="-2.8973" upper="2.8973" velocity="2.6100"/>
  </joint>
  <joint name="ry_joint" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="ry"/>
    <child link="rz"/>
    <axis xyz="0 1 0"/>
    <limit effort="12" lower="-2.8973" upper="2.8973" velocity="2.6100"/>
  </joint>
  <joint name="rz_joint" type="revolute">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="rz"/>
    <child link="mug"/>
    <axis xyz="0 0 1"/>
    <limit effort="12" lower="-2.8973" upper="2.8973" velocity="2.6100"/>
  </joint>

  <link name="mug">
    <visual>
      <origin xyz="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="mug.obj" scale="1 1 1"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename="mug.obj" scale="1 1 1"/>
      </geometry>
    </collision>

    <inertial>
      <mass value="0.0"/>
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
    </inertial>
  </link>
</robot>
